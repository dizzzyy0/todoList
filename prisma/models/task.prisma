model Task {
  id          String  @id @default(uuid()) @db.Uuid
  title       String
  description String? 
  status TaskStatus @default(not_started)
  dueDate   DateTime?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt

  listId String @db.Uuid
  list   List   @relation(fields: [listId], references: [id])
}